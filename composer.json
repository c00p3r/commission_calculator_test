{
  "require": {
    "php": "~8.2.3",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/phpunit/phpunit/phpunit",
    "test-coverage": "XDEBUG_MODE=coverage vendor/phpunit/phpunit/phpunit --coverage-html ./coverage"
  }
}
